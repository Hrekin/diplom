%h1 Listing exemplar_details

%table
  %thead
    %tr
      %th Exemplar detail name
      %th Exemplar detail description
      %th Inventory number
      %th Detail Model
      %th Set detail
      %th
      %th
      %th

  %tbody
    - @exemplar_details.each do |exemplar_detail|
      %tr
        %td= exemplar_detail.exemplar_detail_name
        %td= exemplar_detail.exemplar_detail_description
        %td= exemplar_detail.inventory_number
        %td= exemplar_detail.detail_model
        %td= exemplar_detail.set_detail
        %td= link_to 'Show', exemplar_detail
        %td= link_to 'Edit', edit_exemplar_detail_path(exemplar_detail)
        %td= link_to 'Destroy', exemplar_detail, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Exemplar detail', new_exemplar_detail_path
